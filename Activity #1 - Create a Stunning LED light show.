//Activity #1 - Create a Stunning LED light show.

void setup()
{
  pinMode(13, OUTPUT);
  pinMode(12, OUTPUT);
  pinMode(11, OUTPUT);
  pinMode(10, OUTPUT);
  pinMode(9, OUTPUT);
  pinMode(8, OUTPUT);
  pinMode(7, OUTPUT);
  pinMode(6, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(4, OUTPUT);
}

void loop()
{
  digitalWrite(13, HIGH);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(13, LOW);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(12, HIGH);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(12, LOW);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(11, HIGH);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(11, LOW);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(10, HIGH);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(10, LOW);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(9, HIGH);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(9, LOW);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(8, HIGH);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(8, LOW);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(7, HIGH);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(7, LOW);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(6, HIGH);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(6, LOW);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(5, HIGH);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(5, LOW);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(4, HIGH);
  delay(1000); // Wait for 1000 * speed millisecond(s)
  digitalWrite(4, LOW);
  delay(1000); // Wait for 1000 * speed millisecond(s)
}
